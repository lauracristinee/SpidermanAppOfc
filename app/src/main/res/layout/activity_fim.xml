<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundofim"
    tools:context=".fim">

    <TextView
        android:id="@+id/txtparabens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="120dp"
        android:width="200dp"
        android:height="60dp"
        android:gravity="center"
        android:text="@string/txtparabens"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/imgsm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnvoltainicio"
        android:layout_width="134dp"
        android:layout_height="60dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="7dp"
        android:backgroundTint="@color/design_default_color_error"
        android:text="INÍCIO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnvoltamenu" />

    <Button
        android:id="@+id/btnvoltamenu"
        android:layout_width="134dp"
        android:layout_height="60dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="664dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="7dp"
        android:backgroundTint="@color/design_default_color_error"
        android:text="MENU"
        android:textColorHighlight="@color/design_default_color_primary"
        android:textColorLink="@color/design_default_color_primary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnvoltainicio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>